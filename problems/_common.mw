<Group hide-output="false" labelreference="L4109" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### cross product matrix
cross := v-&gt;&lt;0, -v[3], v[2]; v[3], 0, -v[1]; -v[2], v[1], 0&gt;:</Text-field>
</Input>
</Group>
<Group hide-output="false" labelreference="L4108" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### rotation matrix by the Cayley transform
getRotMatrix := v-&gt;&lt;v[1]^2-v[2]^2-v[3]^2+1, 2*(v[1]*v[2]+v[3]), 2*(v[1]*v[3]-v[2]);
                   2*(v[1]*v[2]-v[3]), -v[1]^2+v[2]^2-v[3]^2+1, 2*(v[2]*v[3]+v[1]);
                   2*(v[1]*v[3]+v[2]), 2*(v[2]*v[3]-v[1]), -v[1]^2-v[2]^2+v[3]^2+1&gt;/~(v[1]^2+v[2]^2+v[3]^2+1):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4118" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### fundamental matrix from 2 camera matrices
getFundMatrix := (P1, P2)-&gt;Matrix(3, (j,i)-&gt;(-1)^(i+j)*LinearAlgebra[Determinant](&lt;LinearAlgebra[DeleteRow](P1, i), LinearAlgebra[DeleteRow](P2, j)&gt;)):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4132" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### trifocal tensor from 3 camera matrices
getTrifocalTensor := (P1, P2, P3)-&gt;map(k-&gt;Matrix(3, (i,j)-&gt;(-1)^(k+1)*LinearAlgebra[Determinant](&lt;LinearAlgebra[DeleteRow](P1, k), P2[i,..], P3[j,..]&gt;)), [1,2,3]):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4129" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### quaternion to rotation matrix transform
quat2rot := q-&gt;&lt;q[1]^2+q[2]^2-q[3]^2-q[4]^2,2*q[2]*q[3]-2*q[1]*q[4],2*q[1]*q[3]+2*q[2]*q[4]; 
                2*q[1]*q[4]+2*q[2]*q[3],q[1]^2-q[2]^2+q[3]^2-q[4]^2,2*q[3]*q[4]-2*q[1]*q[2];
                2*q[2]*q[4]-2*q[1]*q[3],2*q[1]*q[2]+2*q[3]*q[4],q[1]^2-q[2]^2-q[3]^2+q[4]^2&gt;:</Text-field>
</Input>
</Group>
<Group hide-output="false" labelreference="L4106" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### sort wrt monomial ordering
srt := (V, tord)-&gt;sort(V, (x, y)-&gt;Groebner[TestOrder](y, x, tord)):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4128" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### support of Laurent polynomial f
supp := (pl, vars)-&gt;Groebner[Support](expand(numer(pl)), vars)/~Groebner[Support](denom(pl), vars)[1]:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4127" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### support of Laurent polynomials from F
Supp := proc(F, vars)
    local SF := map(supp, F, vars):
    return SF, {map(op, SF)[]}:
end proc:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4125" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### transform list of Laurent polynomials into coefficient (Macaulay) matrix and monomial vector
poly2matrix := proc(F, vars)
    local T := map(pl-&gt;table(zip((a,b)-&gt;b=a, [coeffs(pl, vars, 'mn')], [mn])), F), B := [Supp(F, vars)[2][]]:
    return Matrix(map(t-&gt;map(b-&gt;`if`(assigned(t[b]), t[b], 0), B), T)), Vector(B):
end proc:</Text-field>
</Input>
</Group>
<Group hide-output="false" labelreference="L4101" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### permute columns of matrix M
permuteColumns := (M, V, V1)-&gt;local mn: M[..,[seq(ListTools[Search](mn, V), mn in V1)]]:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4121" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### construct matrix with the same pattern of non-zero entries
samePattern := proc(M::Matrix, sym::name, xcl::set:={})
    local m := op([1,1], M), n := op([1,2], M), Mt, iM, eM:
    Mt := convert(M, table):
    iM := [indices(Mt, indexorder)]:
    eM := map(i-&gt;`if`(M[iM[i][]] in xcl, M[iM[i][]], sym(iM[i][1]+(iM[i][2]-1)*m-m^2)), [$ 1..numelems(iM)]):
    return Matrix(m, n, {zip((x,y)-&gt;x[]=y, iM, eM)[]}):
end proc:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4131" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### Groebner basis of polynomial ideal
gb := (F, tord, pp)-&gt;Groebner[Basis](F, tord, characteristic=pp):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4130" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### dimension of polynomial ideal
dim := (F, tord, pp)-&gt;Groebner[HilbertDimension](F, tord, characteristic=pp):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4122" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### degree of polynomial ideal
deg := proc(F, tord, pp, indvars:={})
    local F1 := subs(map(vr-&gt;vr=rnd(), indvars), F), vars1 := indets(tord) minus {tord} minus indvars, tord1 := tdeg(vars1[]):
    return numelems(Groebner[NormalSet](gb(F1, tord1, pp), tord1)[1]):
end proc:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4133" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### find roots at infinity
inftyRoots := proc(F, tord, pp)
    local Gr, hGr, h:
    Gr := gb(F, tord, pp):
    hGr := Groebner[Homogenize](Gr, h):
    return msolve(subs(h=0, {hGr[]}), pp):
end proc:</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L4123" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### save polynomial system and (optional) procedure for constructing coefficients
saveF := proc(prob, F, C_prob:=NULL)
    local fn := cat(&quot;F_&quot;, prob, &quot;.m&quot;):
    cat(F_, prob) := F:
    if C_prob=NULL then
        save cat(F_, prob), fn:
    else
        save cat(F_, prob), C_prob, fn:
    end if:
end proc:</Text-field>
</Input>
</Group>
