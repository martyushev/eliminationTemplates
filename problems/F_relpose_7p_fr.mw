<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### compute coefficient matrix of initial polynomial system
cat(C_, prob) := proc({symbolic:=1})
    _name, _size := [`p1`, `p2`], [[2,7] $ 2]:
    if symbolic=0 then
        _A := [seq(rndMat(_size[i][]), i=1..nops(_name))]:
    else
        _A := [seq(Matrix(_size[i][], (x,y)-&gt;_name[i](x+(y-1)*_size[i][1])), i=1..nops(_name))]:
    end if:
    
    ################## place your code for constructing polynomials here
    for i from 1 to 7 do
        pu1[i] := &lt;_A[1][1,i], _A[1][2,i], 1+lambda*(_A[1][1,i]^2+_A[1][2,i]^2)&gt;:
        pu2[i] := &lt;_A[2][1,i], _A[2][2,i], 1+lambda*(_A[2][1,i]^2+_A[2][2,i]^2)&gt;:
    end do:
    F := subs(f[3,3]=1, Matrix(3, symbol=f)):
    eqs := map(expand, [seq(pu1[i]^+.F.pu2[i], i=1..7)]):
    vars := [lambda, f[1,1], f[1,2], f[1,3], f[2,1], f[2,2], f[2,3], f[3,1], f[3,2]]:
    M, V := poly2matrix(eqs, vars):
    V1 := &lt;f[1,1], f[2,1], f[1,2], f[2,2], f[3,1], f[3,1]*lambda, lambda^2, f[3,2], f[3,2]*lambda, f[1,3], lambda*f[1,3], f[2,3], f[2,3]*lambda, lambda, 1&gt;:
    M1 := permuteColumns(M, V, V1):
    if symbolic=0 then
        tM1 := LinearAlgebra[ReducedRowEchelonForm](M1):
        #print(convert(samePattern(tM1, A, {1,-1}), listlist)):
    else
        tM1 := Matrix([[1, 0, 0, 0, 0, 0, 0, A(1), A(8), A(15), A(22), A(29), A(36), A(43), A(50)], [0, 1, 0, 0, 0, 0, 0, A(2), A(9), A(16), A(23), A(30), A(37), A(44), A(51)], [0, 0, 1, 0, 0, 0, 0, A(3), A(10), A(17), A(24), A(31), A(38), A(45), A(52)], [0, 0, 0, 1, 0, 0, 0, A(4), A(11), A(18), A(25), A(32), A(39), A(46), A(53)], [0, 0, 0, 0, 1, 0, 0, A(5), A(12), A(19), A(26), A(33), A(40), A(47), A(54)], [0, 0, 0, 0, 0, 1, 0, A(6), A(13), A(20), A(27), A(34), A(41), A(48), A(55)], [0, 0, 0, 0, 0, 0, 1, A(7), A(14), A(21), A(28), A(35), A(42), A(49), A(56)]]):
    end if:
    sf[1,1] := V1[1]-tM1[1,..].V1:
    sf[2,1] := V1[2]-tM1[2,..].V1:
    sf[1,2] := V1[3]-tM1[3,..].V1:
    sf[2,2] := V1[4]-tM1[4,..].V1:
    sf[3,1] := V1[5]-tM1[5,..].V1:
    omega := LinearAlgebra[DiagonalMatrix]([1, 1, p]):
    cond := [seq(seq(f[i,j]=sf[i,j], i=1..2), j=1..2), f[3,1]=sf[3,1]]:
    eqs := subs(cond, [tM1[6,..].V1, tM1[7,..].V1, LinearAlgebra[Determinant](F), convert(2*F.omega.F^+.omega.F-LinearAlgebra[Trace](omega.F.omega.F^+)*F, list)[]]):
    eqs := subs([p=v, f[3,2]=w, f[1,3]=x, f[2,3]=y, lambda=z], eqs):
    ##################

    _F := map(expand, eqs):
    if symbolic=0 then return _F: end if:
    _vars := indets(_F) minus indets(_A):
    _C0, _V0 := poly2matrix(_F, _vars):
    _V := srt(_V0, tdeg(_vars[])):
    [M1, permuteColumns(_C0, _V0, _V)], _V, _name, _size:
end proc:</Text-field>
</Input>
